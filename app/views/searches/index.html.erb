<div class="flex-container-body" id="user-profile">
  <div class="flex-container-profile">
    <div class="flex-container" id="logo-styling">
      <a href="<%= edit_user_path(@dashboard.user) %>" class="flex-item-center" id="current-user-name" data-user="<%= @dashboard.user.id %>"><%= full_name(@dashboard.user) %></a>

    </div>

    <img src="/images/profile.jpg" class="footer-image">

    <%= render partial: "users/follow_stats", locals: {dashboard: @dashboard} %>
    <%= render partial: "shared/rant_form", locals: {dashboard: @dashboard} %>


  </div>

  <div class="flex-container-user-body" id="user-body">
    <div class="flex-container-search-rant-form">
      <%= render partial: "search_form", locals: {users: @users, rants: @rants} %>
    </div>
  </div>
</div>