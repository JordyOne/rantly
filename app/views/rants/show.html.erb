<div class="flex-container-body" id="user-profile">
  <div class="flex-container-profile">
    <div class="flex-container" id="logo-styling">
      <a href="<%= edit_user_path %>" class="flex-item-center" id="current-user-name" data-currentuser="<%= @user.id %>"><%= full_name(@user) %></a>

    </div>

    <img src="/images/profile.jpg" class="footer-image">

    <%= render partial: 'shared/rant_form', locals: {dashboard: @dashboard} %>

  </div>
  <div class="flex-container-user-body" id="user-body">
    <div class="rant-body"><%= render partial: "show_rant", locals: {dashboard: @dashboard} %></div>

  </div>
</div>